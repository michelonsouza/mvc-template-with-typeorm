{
  "name": "devcontainer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "init:ormconfig": "yarn prebuild",
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "NODE_ENV=production node dist/server.js",
    "prebuild": "ts-node-dev ./src/database/init-ormconfig.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:create": "yarn typeorm migration:create -c seed -n",
    "seed:run": "yarn typeorm migration:run -c seed",
    "seed:revert": "yarn typeorm migration:revert -c seed"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.4.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.7",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
